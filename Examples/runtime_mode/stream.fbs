namespace arm_cmsis_stream;


struct UUID {
   v:[ubyte:16];
}

struct IODesc {
   id:uint16;
   nb:uint16;
}

table Node {
   uuid:UUID;
   id:uint16;
   inputs:[IODesc];
   outputs:[IODesc];
   node_data:[byte];
   name:string;
}

table FIFODesc {
   id:uint16;
   length:uint32;
   delay:uint32;
   buffer:bool;
}

table Schedule {
    async_mode:bool;
    nodes:[Node];
    fifos:[FIFODesc];
    schedule:[uint16];
}


root_type Schedule;